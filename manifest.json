{
    "manifest_version": 3,
    "version":"0.0.1",
    "name": "EMODO",
    "permissions": [
        "storage",
        "notifications"
      ],
    "description": "This my todo that i am creating using Chrome extension",
    "action":{
"default_popup":"popup.html",
"content_security_policy": "script-src 'self'; object-src 'self'"
,
"default_icon":{

    "16":"icons/icon.png",
    "48":"icons/icon.png",
    "128":"icons/icon.png"


},
"background":{
    "service_worker":"background.js"
}

    }
}